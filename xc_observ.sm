        read < jd_SMARTS_r 7 mag_SMARTS_r 8 >
	set mjd_SMARTS_r = jd_SMARTS_r - 2400000.5
	set lg_f_SMARTS_r = -0.4*mag_SMARTS_r 

	#set mjd_$name  = mjd_SMARTS_r
