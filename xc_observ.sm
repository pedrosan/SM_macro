        set nwha = < 25.974    >
        set fwha = < 13.8e-11 >
        set ewha = < 1.67e-11  >
        foreach 6 < n f e > {
           set $6_w94_h = $6wha
        }
        
        #---------------------------------------
        # Whipple: 1995 April 26 (high-b)
        #
        set nwhb = < 25.898    >
        set fwhb = < 7.71e-11  >
        set ewhb = < 0.474e-11 >
        foreach 6 < n f e > {
           set $6_w95_h = $6whb
        }
        
        #---------------------------------------
        # Whipple: 1994/1995 average
        #
        set nwqb = < 25.885   >
        set fwqb = < 3.13e-11 >
        set ewqb = < 0.14e-11 >
        foreach 6 < n f e > {
