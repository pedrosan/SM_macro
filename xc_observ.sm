        # moved xte96_bowtie to 1996 section
        foreach 9 < asca93 asca94 xte96 > {
           set nfn_$9 = lg(f_$9)+$surf_421
           ltype 1 
           connect n_$9 nfn_$9
           ltype 0
           points  n_$9 nfn_$9 
           logerr  n_$9 nfn_$9 e_$9
